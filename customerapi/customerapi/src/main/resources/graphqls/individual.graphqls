schema {
    query: Query  
  
}


interface Customer  {
    customerId: ID!
	name: FullName
	email: String
	password: String
	contactNo: Integer
}

type Individual implements Customer {
    customerId: ID!
	name: FullName
	email: String
	password: String
	contactNo: Integer
	dob: String
	gender:String
}




type FullName {

	firstName: String
	lastName: String
	middleName: String

}



type Query {
	findAllIndividuals:[Individual]
	findIndividual(customerId: ID):Individual
}


